global coins = int[]{1, 2, 5, 10, 20, 50, 100, 200};

int change(int index, int amount) {
  if (amount == 0) {
    return 1;
  }
  if (index < 0 | amount < 0) {
    return 0;
  }
  return change(index - 1, amount) + change(index, amount - coins[index]);
}

int program(int argc, string[] argv) {
  var r = change(7, 200);
  return r;
}